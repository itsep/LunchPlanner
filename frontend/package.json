{
  "name": "webpack-4-quickstart",
  "version": "1.0.0",
  "description": "> Webpack 4 tutorial: All You Need to Know, from 0 Conf to Production Mode",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-with-coverage": "jest --collectCoverage",
    "test-update-snapshot": "jest -u",
    "start": "webpack-dev-server --mode development --open",
    "start-local-network": "webpack-dev-server --mode development --host 0.0.0.0",
    "build": "webpack --mode production",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "jest": {
    "setupFiles": [
      "./src/setupJest.js"
    ],
    "collectCoverage": false,
    "coverageDirectory": "./src/__coverage__",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "moduleNameMapper": {
      "^components/(.*)": "<rootDir>/src/components/$1",
      "^shared/(.*)": "<rootDir>/../shared/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valentinogagliardi/webpack-4-quickstart.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valentinogagliardi/webpack-4-quickstart/issues"
  },
  "homepage": "https://github.com/valentinogagliardi/webpack-4-quickstart#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.43",
    "babel-polyfill": "^6.26.0",
    "cuid": "^2.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-promise": "^4.2.4",
    "expect-enzyme": "^1.3.0",
    "immutable": "^3.8.2",
    "jest-fetch-mock": "^1.5.0",
    "material-ui": "^1.0.0-beta.41",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.3.1",
    "redux": "^4.0.0",
    "react-material-ui-form-validator": "^2.0.0-beta.9"
  }
}
